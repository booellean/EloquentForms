{
    "name": "nickwest/eloquent-forms",
    "description": "Create Bulma compatible forms and tables directly from Eloquent Models and Collections",
    "authors": [
        {
            "name": "Nick West",
            "email": "nickwest@uw.edu"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5|^8.0",
        "illuminate/support": "^7.29",
        "nesbot/carbon": "^2.0",
        "doctrine/dbal": "~2.13",
        "maatwebsite/excel": "^3.1",
        "nickwest/blossom-php": "^1.0"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-4": {
            "Nickwest\\EloquentForms\\": "src/",
            "Nickwest\\EloquentForms\\Test\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "filp/whoops": "^2.0",
        "fakerphp/faker": "^1.9.1",
        "laravel/framework": "^7.29",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.5.8|^9.3.3",

        "orchestra/testbench": "^5.15",
        "orchestra/database": "^5.1",
        "kub-at/php-simple-html-dom-parser": "^1.9"
        },
    "extra": {
        "laravel": {
            "providers": [
                "Nickwest\\EloquentForms\\EloquentFormsServiceProvider",
                "Nickwest\\EloquentForms\\Themes\\bulma\\EloquentFormsBulmaThemeServiceProvider"
            ]
        }
    }
}
